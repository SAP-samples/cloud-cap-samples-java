   *
   * This is a contact person attribute that you can define in Customizing.Personnel managerSecretary
   */
  @sap.display.format : 'UpperCase'
  @sap.label : 'Function'
  @sap.quickinfo : 'Function of partner'
  ContactPersonFunction : String(4);
