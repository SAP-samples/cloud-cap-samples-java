{
	"welcomeFile": "/app/fiori.html",
	"authenticationMethod": "route",
	"routes": [
	  {
		"source": "^/app/(.*)$",
		"target": "$1", 
		"localDir": "resources",
		"cacheControl": "no-cache, no-store, must-revalidate",
		"authenticationType": "xsuaa"
	  },
	  {
		"source": "(/.*)/webapp/(.*)$",
		"localDir": "resources",
		"authenticationType": "xsuaa"
	  },
	  {
		"source": "(/.*)/admin(/.*)",
		"authenticationType": "xsuaa",
		"destination": "backend"
	  },
	  {
		"source": "^/(.*)$",
		"authenticationType": "none",
		"destination": "backend"
	  }
	]
}
